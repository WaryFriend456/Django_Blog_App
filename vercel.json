{
    "builds": [{
        "src": "djangoProject/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": { "Cache-Control": "max-age=31536000, immutable" },
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "djangoProject/wsgi.py"
        }
    ]
}